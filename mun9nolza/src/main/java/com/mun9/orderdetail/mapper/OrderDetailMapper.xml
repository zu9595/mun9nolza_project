<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.mun9.orderdetail.mapper.OrderDetailMapper">
<insert id="insertOrderDetail" parameterType="cart">
	insert into orderdetail values(#{proCode}, order_no_seq.currval, #{myproCnt}, #{myproCnt}*#{myproPrice})
</insert>

<select id="selectOrderDetailList" resultType="orderdetailT">
	select * from orderdetail order by 2
</select>
</mapper>